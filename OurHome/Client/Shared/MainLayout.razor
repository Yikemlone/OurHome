@inherits LayoutComponentBase
@inject ISnackbar Snackbar

<MudThemeProvider/>
<MudDialogProvider />
<MudSnackbarProvider />

<MudLayout> 

    <MudAppBar Elevation="1" Color="Color.Secondary">
        <MudText Typo="Typo.h6"><b>Our</b>Home</MudText>

        <MudSpacer/>

        <MudIconButton Icon="@Icons.Material.Outlined.Coffee" Color="MudBlazor.Color.Inherit"/>
    </MudAppBar>

    <MudMainContent>
        <MudContainer MaxWidth="MaxWidth.Large" Class="my-16 pt-16">
            @Body
        </MudContainer>
    </MudMainContent>

</MudLayout>